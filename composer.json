{
    "name": "graham-campbell/exceptions",
    "description": "Provides A Powerful Error Response System For Both Development And Production",
    "keywords": ["laravel", "framework", "exceptions", "exception", "errors", "error", "whoops", "Exceptions", "Laravel Exceptions", "Laravel-Exceptions", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "hello@gjcampbell.co.uk",
            "homepage": "https://github.com/GrahamCampbell"
        }
    ],
    "require": {
        "php": "^7.2.5 || ^8.0",
        "illuminate/contracts": "^7.0 || ^8.0",
        "illuminate/support": "^7.0 || ^8.0",
        "psr/log": "^1.1",
        "symfony/console": "^5.0",
        "symfony/error-handler": "^5.0",
        "symfony/http-foundation": "^5.0"
    },
    "require-dev": {
        "filp/whoops": "^2.7.1",
        "graham-campbell/analyzer": "^3.0",
        "graham-campbell/testbench": "^5.6",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5.8 || ^9.3.7"
    },
    "suggest": {
        "filp/whoops": "Enables use of the debug displayer."
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\Exceptions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\Exceptions\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "GrahamCampbell\\Exceptions\\ExceptionsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
