{
    "name": "graham-campbell/exceptions",
    "description": "Provides A Powerful Error Response System For Both Development And Production",
    "keywords": ["laravel", "framework", "exceptions", "exception", "errors", "error", "whoops", "Exceptions", "Laravel Exceptions", "Laravel-Exceptions", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/contracts": "^5.5|^6.0",
        "illuminate/support": "^5.5|^6.0",
        "symfony/console": "^3.3|^4.0|^5.0",
        "symfony/debug": "^3.3|^4.0|^5.0",
        "symfony/http-foundation": "^3.3|^4.0|^5.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "filp/whoops": "^2.4",
        "graham-campbell/analyzer": "^2.1",
        "graham-campbell/testbench": "^5.2",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5|^7.0|^8.0"
    },
    "suggest": {
        "filp/whoops": "Enables use of the debug displayer."
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\Exceptions\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/stubs/"],
        "psr-4": {
            "GrahamCampbell\\Tests\\Exceptions\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "12.1-dev"
        },
        "laravel": {
            "providers": [
                "GrahamCampbell\\Exceptions\\ExceptionsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
