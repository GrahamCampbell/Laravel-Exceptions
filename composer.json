{
    "name": "graham-campbell/exceptions",
    "description": "Provides A Powerful Error Response System For Both Development And Production",
    "keywords": ["laravel", "framework", "exceptions", "exception", "errors", "error", "whoops", "Exceptions", "Laravel Exceptions", "Laravel-Exceptions", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "5.1.*|5.2.*",
        "illuminate/support": "5.1.*|5.2.*",
        "paragonie/random_compat": "^1.1",
        "symfony/debug": "^2.7|^3.0",
        "symfony/http-foundation": "^2.7|^3.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "filp/whoops": "^1.1",
        "graham-campbell/testbench": "^3.1",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^4.8|^5.0"
    },
    "suggest": {
        "filp/whoops": "Enables use of the debug displayer."
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\Exceptions\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/stubs/"],
        "psr-4": {
            "GrahamCampbell\\Tests\\Exceptions\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
